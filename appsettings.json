{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server={Server};Port={Port};User Id={postgres};Password={Password};Database={Database};"
  },
  "Listener": {
    "BasePort": 8080
  },
  "translateFormat": "PLAIN_TEXT",
  "folderId": "{folderId}",
  "speller": false,
  "HttpClients": [
    {
      "clientId": "YandexClient_KeyApi",
      "clientURI": "https://iam.api.cloud.yandex.net",
      "clientEndpoint": "/iam/v1/tokens",
      "clientAgent": "Person KEY API"
    },
    {
      "clientId": "YandexClient_CloudApi",
      "clientURI": "https://translate.api.cloud.yandex.net",
      "clientEndpoint": "/translate/v2/translate",
      "clientAgent": "Person KEY API"
    }
  ],
  "cachTimeout": 30,
  "yandexPassportOauthToken": "{yandexPassportOauthToken}"
}